DEFINE_ACTION_FUNCTION setup_bg2_items_in_bg1
BEGIN
  ACTION_IF GAME_IS "bgee"
  BEGIN
    LAUNCH_ACTION_FUNCTION run
      STR_VAR
      file	= "missing_bgee_items"
      tra	= "missing_bgee_items"
    END
  END

  LAUNCH_ACTION_FUNCTION run
    STR_VAR
    file	= "missing_bgee_items"
    tra		= "missing_bgee_items"
  END
END	// setup_bg2_items_in_bg1


